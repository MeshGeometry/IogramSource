<elements>

	<!-- Basic 9 slice UI image -->
	<element type="BorderImage">
        <attribute name="Blend Mode" value="alpha" />
        <attribute name="Color" value="0.85 0.85 0.85" />
        <attribute name="Texture" value="Texture2D;Textures/Box_WR2.png" />
        <attribute name="Image Rect" value="0 0 32 32" />
        <attribute name="Border" value="4 4 4 4" />
    </element>

    <!-- Basic Button -->
    <element type="Button" style="BorderImage">
        <attribute name="Texture" value="Texture2D;Textures/Box_WR4.png" />
        <attribute name="Pressed Image Offset" value="-1 -1" />
        <attribute name="Hover Image Offset" value="1 1" />
        <attribute name="Border" value="6 6 6 6" />
        <attribute name="Color" value="0.5 0.5 0.85" />
    </element>

    <!-- Basic Button -->
    <element type="HoverButton" style="Button">
        <attribute name="Texture" value="Texture2D;Textures/Box_WR4.png" />
        <attribute name="Pressed Image Offset" value="-1 -1" />
        <attribute name="Hover Image Offset" value="1 1" />
        <attribute name="Border" value="6 6 6 6" />
        <attribute name="Color" value="0.5 0.5 0.85" />
    </element>

        <!-- Icon Button -->
    <element type="IconButton" style="HoverButton">
        <attribute name="Texture" value="Texture2D;Textures/Close.png" />
        <attribute name="Image Rect" value="0 0 32 32" />
        <attribute name="Size" value = "24 24"/>
        <attribute name="Border" value="0 0 0 0" />
        <attribute name="Focus Mode" value="NotFocusable" />
    </element>

        <!-- Close Button -->
    <element type="CloseButton" style="HoverButton">
        <attribute name="Texture" value="Texture2D;Textures/Close.png" />
        <attribute name="Image Rect" value="0 0 32 32" />
        <attribute name="Size" value = "16 16"/>
        <attribute name="Border" value="0 0 0 0" />
        <attribute name="Focus Mode" value="NotFocusable" />
    </element>

    <!-- Text-->
    <element type="Text">
        <attribute name="Font" value="Font;Fonts/lato-regular.ttf" />
        <attribute name="Font Size" value="9" />
        <attribute name="Color" value="0.85 0.85 0.85" />
    </element>

    <!-- Text-->
    <element type="LabelText">
        <attribute name="Font" value="Font;Fonts/lato-regular.ttf" />
        <attribute name="Font Size" value="16" />
        <attribute name="Use Derived Opacity" value="false"/>
        <attribute name="Text Effect" value="Shadow" />
        <attribute name="Color" value="0.85 0.85 0.85" />
    </element>

    <!-- Text-->
    <element type="SmallLabelText">
        <attribute name="Font" value="Font;Fonts/lato-regular.ttf" />
        <attribute name="Font Size" value="12" />
        <attribute name="Use Derived Opacity" value="false"/>
        <attribute name="Text Effect" value="Shadow" />
        <attribute name="Color" value="0.85 0.85 0.85" />
    </element>

        <!-- Text-->
    <element type="TinyLabelText">
        <attribute name="Font" value="Font;Fonts/lato-regular.ttf" />
        <attribute name="Font Size" value="8" />
        <attribute name="Use Derived Opacity" value="false"/>
        <attribute name="Text Effect" value="Shadow" />
        <attribute name="Color" value="0.5 0.5 0.5" />
    </element>

    <!-- Cursor Shapes -->
    <element type="Cursor">
        <attribute name="Shapes">
            <variant type="VariantVector" >
                <variant type="String" value="Normal" />
                <variant type="ResourceRef" value="Image;Textures/UI.png" />
                <variant type="IntRect" value="0 0 12 24" />
                <variant type="IntVector2" value="0 0" />
            </variant>
            <variant type="VariantVector" >
                <variant type="String" value="ResizeVertical" />
                <variant type="ResourceRef" value="Image;Textures/UI.png" />
                <variant type="IntRect" value="0 64 20 84" />
                <variant type="IntVector2" value="9 9" />
            </variant>
            <variant type="VariantVector" >
                <variant type="String" value="ResizeDiagonalTopRight" />
                <variant type="ResourceRef" value="Image;Textures/UI.png" />
                <variant type="IntRect" value="20 64 40 84" />
                <variant type="IntVector2" value="9 9" />
            </variant>
            <variant type="VariantVector" >
                <variant type="String" value="ResizeHorizontal" />
                <variant type="ResourceRef" value="Image;Textures/UI.png" />
                <variant type="IntRect" value="40 64 60 84" />
                <variant type="IntVector2" value="9 9" />
            </variant>
            <variant type="VariantVector" >
                <variant type="String" value="ResizeDiagonalTopLeft" />
                <variant type="ResourceRef" value="Image;Textures/UI.png" />
                <variant type="IntRect" value="60 64 80 84" />
                <variant type="IntVector2" value="9 9" />
            </variant>
            <variant type="VariantVector" >
                <variant type="String" value="RejectDrop" />
                <variant type="ResourceRef" value="Image;Textures/UI.png" />
                <variant type="IntRect" value="80 64 100 84" />
                <variant type="IntVector2" value="9 9" />
            </variant>
            <variant type="VariantVector" >
                <variant type="String" value="AcceptDrop" />
                <variant type="ResourceRef" value="Image;Textures/UI.png" />
                <variant type="IntRect" value="100 64 128 90" />
                <variant type="IntVector2" value="0 0" />
            </variant>
            <variant type="VariantVector" >
                <variant type="String" value="Busy" />
                <variant type="ResourceRef" value="Image;Textures/UI.png" />
                <variant type="IntRect" value="128 64 148 85" />
                <variant type="IntVector2" value="9 9" />
            </variant>
        </attribute>
    </element>

    <!-- Slider-->
    <element type="Slider" style="BorderImage">
        <attribute name="Size" value="200 12" />
        <attribute name="Texture" value="Texture2D;Textures/Box_GRR4.png" />
        <attribute name="Image Rect" value="0 0 32 32" />
        <attribute name="Border" value="6 6 6 6" />
        <attribute name="Range" value="100"/>
        <attribute name="Color" value="0.4 0.4 0.4 1.0"/>
        <element type="BorderImage" internal="true">
            <attribute name="Texture" value="Texture2D;Textures/Box_GRR4.png" />
            <attribute name="Image Rect" value="0 0 32 32" />
            <attribute name="Border" value="6 6 6 6" />
            <attribute name="Min Size" value="20 10" />
            <attribute name="Hover Image Offset" value="0 0" />
        </element>
    </element>

    <!-- Slider-->
    <element type="ThinSlider" style="BorderImage">
        <attribute name="Size" value="200 12" />
        <attribute name="Texture" value="Texture2D;Textures/Box_GRR4.png" />
        <attribute name="Image Rect" value="0 0 32 32" />
        <attribute name="Border" value="6 6 6 6" />
        <attribute name="Range" value="100"/>
        <attribute name="Color" value="0.3 0.3 0.3 0.5"/>
        <attribute name="Blend Mode" value="alpha"/>
        <element type="BorderImage" internal="true">
            <attribute name="Texture" value="Texture2D;Textures/Box_WR4.png" />
            <attribute name="Image Rect" value="0 0 32 32" />
            <attribute name="Border" value="6 6 6 6" />
            <attribute name="Min Size" value="10 10" />
            <attribute name="Color" value="0.27 0.27 0.27 0.5"/>
            <attribute name="Blend Mode" value="alpha"/>
        </element>
    </element>

    <!-- Slider-->
    <element type="ScrollBar">
        <attribute name="Min Size" value="12 12" />
        <element type="Button" internal="true">
            <attribute name="Size" value="12 12" />
            <attribute name="Border" value="4 4 4 4" />
            <attribute name="Is Visible" value="false"/>
        </element>
        <element type="Slider" style="ThinSlider" internal="true">
            <attribute name="Size" value="12 12" />
        </element>
        <element type="Button" internal="true">
            <attribute name="Size" value="12 12" />
            <attribute name="Border" value="4 4 4 4" />
            <attribute name="Is Visible" value="false"/>
        </element>
    </element>

    <!-- Line Edit -->
    <element type="LineEdit" style="BorderImage">
        <attribute name="Border" value="4 4 4 4" />
        <attribute name="Image Rect" value="0 0 32 32" />
        <attribute name="Hover Image Offset" value="0 0" /> 
        <atrribute name="Use Derived Opacity" value="false"/>
        <attribute name="Min Size" value="10 24" />
        <attribute name="Max Size" value="2147483647 40" />
        <attribute name="Clip Border" value="4 4 4 4" />
        <attribute name="Color" value="0.3 0.3 0.3 1.0"/>
        <element type="Text" internal="true">
            <attribute name="Color" value="0.9 1 0.9 1" />
            <attribute name="Font Size" value="12"/>
            <attribute name="Selection Color" value="0.3 0.4 0.7 1" />
        </element>
        <element type="BorderImage" internal="true">
            <attribute name="Size" value="4 16" />
            <attribute name="Priority" value="10000" />
            <attribute name="Texture" value="Texture2D;Textures/Box_WR0.png"/>
            <attribute name="Image Rect" value="0 0 32 32" />
            <attribute name="Color" value="0.0 0.8 0.8 1.0"/>
        </element>
    </element>

        <!-- Line Edit -->
    <element type="ClickEditText" style="BorderImage">
        <attribute name="Border" value="4 4 4 4" />
        <attribute name="Image Rect" value="0 0 32 32" />
        <attribute name="Hover Image Offset" value="0 0" /> 
        <atrribute name="Use Derived Opacity" value="false"/>
        <attribute name="Min Size" value="10 24" />
        <attribute name="Max Size" value="2147483647 40" />
        <attribute name="Clip Border" value="4 4 4 4" />
        <attribute name="Color" value="0.3 0.3 0.6 1.0"/>
        <element type="Text" internal="true">
            <attribute name="Color" value="0.9 1 0.9 1" />
            <attribute name="Font Size" value="9"/>
            <attribute name="Selection Color" value="0.3 0.4 0.7 1" />
        </element>
        <element type="BorderImage" internal="true">
            <attribute name="Size" value="1 16" />
            <attribute name="Priority" value="10000" />
            <attribute name="Texture" value=""/>
            <attribute name="Image Rect" value="0 0 32 32" />
            <attribute name="Color" value="0.8 0.8 0.8 1.0"/>
        </element>
    </element>

    <!-- Line Edit -->
    <element type="MultiLineEdit" style="BorderImage">
        <attribute name="Border" value="4 4 4 4" />
        <attribute name="Image Rect" value="0 0 32 32" />
        <attribute name="Hover Image Offset" value="0 0" /> 
        <atrribute name="Use Derived Opacity" value="false"/>
        <attribute name="Min Size" value="10 30" />
        <attribute name="Max Size" value="2147483647 2147483647" />
        <attribute name="Clip Border" value="2 2 2 2" />
        <attribute name="Color" value="0.3 0.3 0.3 1.0"/>
        <element type="Text" internal="true">
            <attribute name="Color" value="0.9 1 0.9 1" />
            <attribute name="Font Size" value="12"/>
            <attribute name="Selection Color" value="0.3 0.4 0.7 1" />
        </element>
        <element type="BorderImage" internal="true">
            <attribute name="Size" value="1 16" />
            <attribute name="Priority" value="10000" />
            <attribute name="Texture" value=""/>
            <attribute name="Image Rect" value="0 0 32 32" />
            <attribute name="Color" value="0.8 0.8 0.8 1.0"/>
        </element>
    </element>


    <!-- Scroll View -->
    <element type="ScrollView">
        <element type="ScrollBar" internal="true">
            <attribute name="Use Derived Opacity" value="false"/>
            <attribute name="Size" value="0 8" />
        </element>
        <element type="ScrollBar" internal="true">
            <attribute name="Use Derived Opacity" value="false"/>
            <attribute name="Size" value="8 0" />
        </element>
        <element type="BorderImage" internal="true">
            <attribute name="Clip Border" value="2 2 2 2" />
            <attribute name="Color" value="0.1 0.1 0.1 0.1"/>
        </element>
    </element>

    <!-- Window -->
    <element type="Window" style="BorderImage">
        <attribute name="Resize Border" value="8 8 8 8" />
    </element>

    <element type="ListView" style="ScrollView">  <!-- Shortcut to copy all the styles from ScrollView -->
        <element type="BorderImage" internal="true">
            <element internal="true">
                <attribute name="Layout Mode" value="Vertical" />
            </element>
        </element>
    </element>

            <!-- Drop Down -->
    <element type="DropDownList" style="BorderImage">
        <attribute name="Image Rect" value="0 0 32 32" />
        <attribute name="Border" value="4 4 4 4" />
        <attribute name="Layout Mode" value="Horizontal" />
        <attribute name="Layout Border" value="4 4 4 4" />
        <attribute name="Min Size" value="20 12" />
        <attribute name="Max Size" value="2147483647 30" />
        <attribute name="Clip Border" value="5 5 5 5" />
        <element internal="true">
            <element type="SmallLabelText" internal="true" />
        </element>
        <element type="Window" internal="true" popup="true">
            <attribute name="Layout Border" value="5 5 5 5" />
            <attribute name="Color" value="0.1 0.1 0.1 0.9"/>
            <element type="ListView" internal="true">
                <attribute name="Highlight Mode" value="Always" />
                <attribute name="Color" value="0.1 0.1 0.1 0.001"/>
                <element type="BorderImage" internal="true"> 
                    <attribute name="Color" value="0.1 0.1 0.1 0.001"/>
                    <attribute name="Clip Border" value="1 1 1 1" />
                </element>
            </element>
        </element>
    </element>


</elements>